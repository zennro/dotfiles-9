audio/wav;                 /usr/bin/play '%s'
text/html;                 /usr/bin/w3m -T text/html '%s'; description=HTML Text; nametemplate=%s.html
text/html;                 /usr/bin/w3m -dump -T text/html '%s'; copiousoutput; description=HTML Text; nametemplate=%s.html
application/pdf;           mutt_bgrun okular /dev/stdin
application/pdf;           pdftotext %s -; copiousoutput
application/postscript;    mutt_bgrun okular %s
text/calendar;             vcalendar-filter; copiousoutput
